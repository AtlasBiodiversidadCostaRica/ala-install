# A images server installation for a nectar instance. Change the IP address below to your own VM to reuse.

[image-service]
118.138.243.64
  
[image-service:vars]
is_vagrant=false
local_repo_dir = ~/.ala
data_dir=/srv/data
images_url = http://118.138.243.64/images
images_base_url = http://118.138.243.64
images_context_path = /images
